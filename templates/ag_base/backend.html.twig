{% if (app.user != null and 'ROLE_MOBILUSER' in app.user.getRoles()) %}
    {% set baseTag = 'ag_base/base_mobiluser.html.twig' %}
{% elseif (app.user != null and 'ROLE_ADMINOFFICE' in app.user.getRoles()) %}
    {% set baseTag = 'ag_base/base_office.html.twig' %}
{% elseif (app.user != null and 'ROLE_WORKSHOPEMPLOYEE' in app.user.getRoles()) %}
    {% set baseTag = 'ag_base/base_workshop.html.twig' %}
{% endif %}

